$('.mvz').each(function() {
  if ($(this).attr('id') == "mv_<%= @movie.id %>") {
    console.log("y");
    $(this).find('a').addClass('active');
  }else {
    console.log("n");
    $(this).find('a').removeClass('active');
  }
});
